<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"

    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".MainActivity"
    android:orientation="vertical"
    tools:showIn="@layout/app_bar_main">

    <TextView
        android:id="@+id/videopage_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/introhometv"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textSize="20dp"
        android:backgroundTint="@color/diff_del_background"
        />

    <!--android:background="@drawable/ic_launcher_foreground"-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/learnthroughwords"
            android:background="@null"
            android:layout_width="wrap_content"
            android:scaleType="fitXY"
            android:layout_weight="1"
            android:padding="1dp"
            android:adjustViewBounds="true"
            android:layout_height="wrap_content"
            android:src="@drawable/learnthroughwords"/>

        <ImageButton
            android:id="@+id/learnthroughvideos"
            android:background="@null"
            android:layout_width="wrap_content"
            android:scaleType="fitXY"
            android:layout_weight="1"
            android:padding="1dp"
            android:adjustViewBounds="true"
            android:layout_height="wrap_content"
            android:src="@drawable/learnthroughvideos"/>


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal">

        <ImageButton
            android:id="@+id/learnthroughpractice"
            android:background="@null"
            android:layout_width="wrap_content"
            android:scaleType="fitXY"
            android:layout_weight="1"
            android:adjustViewBounds="true"
            android:padding="1dp"
            android:layout_height="wrap_content"
            android:src="@drawable/learnthroughpractice"/>

        <ImageButton
            android:id="@+id/learnthroughquestions"
            android:background="@null"
            android:layout_width="wrap_content"
            android:scaleType="fitXY"
            android:layout_weight="1"
            android:padding="1dp"
            android:adjustViewBounds="true"
            android:layout_height="wrap_content"
            android:src="@drawable/learnthroughquestions"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="vertical">

        <ImageButton
            android:id="@+id/community"
            android:background="@null"
            android:layout_gravity="center_horizontal"
            android:layout_width="wrap_content"
            android:layout_weight="1"
            android:padding="1dp"
            android:scaleType="fitXY"
            android:adjustViewBounds="true"
            android:layout_height="0dp"
            android:src="@drawable/community"/>

    </LinearLayout>


</LinearLayout>

